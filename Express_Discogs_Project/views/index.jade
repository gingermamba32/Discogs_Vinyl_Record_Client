extends layout

block content
	h1= title
	p Welcome to #{title}

	form(name='Insert your search data', method='post', action='/')
		label(for= 'id') Enter your search ID: 
		input(name= 'id', type='text')
		input(type= 'submit', value='submit')


	ul
		each record in records
			li= record.name
			li= record.resource_url

			


